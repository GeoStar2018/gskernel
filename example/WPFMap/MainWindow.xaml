<Window x:Class="WPFMap.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFMap"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid x:Name="MapContainer"></Grid>
        <Label VerticalAlignment="Bottom" x:Name="CoordinateLabel"></Label>
        <StackPanel>

            <Button Click="Button_Click" VerticalAlignment="Top" HorizontalAlignment="Left">开始</Button>
            <Button Click="Button_Open" VerticalAlignment="Top" HorizontalAlignment="Left">Open</Button>
        </StackPanel>
            
    </Grid>
</Window>
